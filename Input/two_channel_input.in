
!********************PROPAGATION PARAMETERS******************************
! final propagation time
total_time: 1d0

! time step size (dt)
time_step_size: 1d0 

! iteration type (options: jacobi, gauss_seidel)
it_type: gauss_seidel

! number of quadrature points
quad_pt: 9

! type of quadrature (newton_cotes or gauss)
quad_type: gauss

! convergence criteria for iteration
it_tolerance: 1.d-10

! cap on number of iterations
it_cap: 30

! example problem (used in data file name only)
example_problem: two_channel

